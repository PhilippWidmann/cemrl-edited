        "sac_layer_size": 300,
        "num_train_epochs": 301,
        "num_train_tasks_per_episode": 20,
        "max_path_length": 200
    },
    "reconstruction_params": {
        "reconstruct_all_timesteps": true,
        "prior_mode": "fixedOnY",
        "prior_sigma": 0.5,
        "num_classes": 1,
        "lr_encoder": 0.0003,
        "lr_decoder": 0.0003,
        "alpha_kl_z": 0,
        "beta_kl_y": 0,
        "alpha_kl_z_query": 0.05,
        "beta_kl_y_query": 0.05,
        "net_complex_enc_dec": 10.0
    }
